  "scripts": {
    "ng": "node_modules/.bin/ng",
    "aot": "node_modules/.bin/ng build --prod --configuration automation --aot true",
    "build": "node_modules/.bin/ng build --prod --configuration production --aot false --build-optimizer false",
    "build:sunrise": "node_modules/.bin/ng build --prod --configuration sunrise --aot false --build-optimizer false",
    "build:auto": "node_modules/.bin/ng build --prod --configuration automation --aot false --build-optimizer false",
    "build:optic": "node_modules/.bin/ng build --prod --configuration optic  --aot false --build-optimizer false",
    "build:dev": "node_modules/.bin/ng build --prod --configuration dev --aot false --build-optimizer false",
    "lint": "node_modules/.bin/ng lint",
    "e2e": "protractor ./protractor.conf.js",
    "hmr": "node_modules/.bin/ng serve --hmr -e=hmr",
    "test:watch": "jest --watch",
    "bundle-report": "webpack-bundle-analyzer dist/stats.json",
    "fix-memory-limit": "cross-env LIMIT=2048 increase-memory-limit",
    "start": "node_modules/.bin/ng serve --open --port 4200",
    "start:webpack": "webpack-dev-server --open",
    "test": "karma start ./karma.conf.js",
    "pree2e": "webdriver-manager update --standalone false --gecko false --quiet"
  },
